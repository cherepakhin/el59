<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ru.el59.office.shopmodel.Discount" table="Discount">
		<id name="n" column="n" type="java.lang.Long" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="val" column="val" type="java.lang.String" />
		<property name="ondetail" column="ondetail" type="java.lang.Boolean" />
		<property name="fromdate" column="fromdate" type="java.sql.Date" />
		<property name="todate" column="todate" type="java.sql.Date" />
	</class>
</hibernate-mapping>