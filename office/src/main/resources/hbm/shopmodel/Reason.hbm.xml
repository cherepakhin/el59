<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ru.el59.office.shopmodel.Reason" table="Reason">
		<id name="n" column="n" type="java.lang.Long" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="isexpense" column="isexpense" type="java.lang.Boolean" />
		<property name="znak" column="znak" type="java.lang.Integer" />
		<property name="korrorder" column="korrorder" type="java.lang.String" />
		<property name="attachment" column="attachment" type="java.lang.String" />
		<property name="cashBookDoc" column="cashBookDoc" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>
